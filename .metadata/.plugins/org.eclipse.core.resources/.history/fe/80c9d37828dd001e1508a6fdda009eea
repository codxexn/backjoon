package day002;

import java.util.Scanner;

public class Q06_2525 {
	
	// 오븐시계 (현재 시간에서 주어진 분 더하기)
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		if (b + c >= 60) {
			a += (b + c) / 60;
			b = (b + c) % 60;
		} else {
			b += c;
		}
		
		if (a > 23) {
			a -= 24;
		}
		
		System.out.println(a);
		System.out.println(b);
		
	}

}
